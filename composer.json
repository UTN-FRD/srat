{
	"name": "utn/srat",
	"description": "Sistema de Registro de Asistencia y Temas",
	"license": "proprietary",
	"authors": [
		{
			"name": "UTN - Facultad Regional Delta",
			"homepage": "http://www.frd.utn.edu.ar/"
		}
	],
	"config": {
		"vendor-dir": "vendor/"
	},
	"repositories": [
		{
			"type": "pear",
			"url": "http://pear.cakephp.org"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"cakedc/search": "2.2.*",
		"ceeram/cakepdf": "1.0.*",
		"pear-pear.cakephp.org/CakePHP": "~2.4.9"
	},
	"require-dev": {
		"cakephp/debug_kit" : "2.2.*",
		"cakephp/localized": "2.1.*",
		"phing/phing": "2.7.0",
		"phpunit/phpunit": "3.7.*"
	},
	"extra": {
		"installer-paths": {
			"src/Plugin/CakePdf": ["ceeram/cakepdf"],
			"src/Plugin/DebugKit": ["cakephp/debug_kit"],
			"src/Plugin/Localized": ["cakephp/localized"],
			"src/Plugin/Search": ["cakedc/search"]
		}
	}
}
